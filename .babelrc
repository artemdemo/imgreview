{
    "presets": [
        "react",
        ["env", {
            "targets": {
                "browsers": ["last 2 versions"]
            }
        }]
    ],
    "plugins": [
        "transform-react-jsx",
        "transform-object-assign",
        "transform-class-properties",
        "transform-object-rest-spread",
        ["transform-runtime", {
            "polyfill": false,
            "regenerator": true
        }]
    ]
}
