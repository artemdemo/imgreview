{
  "name": "imgreview",
  "version": "1.4.0",
  "description": "Image review tool",
  "main": "webpack.config.js",
  "scripts": {
    "start": "./start.sh",
    "server:dev": "webpack-dev-server",
    "server:prod": "NODE_ENV=production npm run build && http-server ./build",
    "build": "webpack --progress --colors",
    "build:prod": "NODE_ENV=production webpack --progress --colors",
    "build:prod:silent": "NODE_ENV=production webpack --silent && npm run postbuild",
    "test": "BABEL_DISABLE_CACHE=1 jest --config ./jest/jest.config.js",
    "stats": "webpack --profile --json > bundle-stats.json && webpack-bundle-analyzer bundle-stats.json -p 8888 -r bundle-report.html",
    "lint": "prettier --write .",
    "deploy": "node deploy.js",
    "postbuild": "react-snap"
  },
  "reactSnap": {
    "source": "./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artemdemo/imgreview.git"
  },
  "author": "Artem Demo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artemdemo/imgreview/issues"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "bootstrap": "^4.5.2",
    "classnames": "^2.2.5",
    "colors": "^1.4.0",
    "final-form": "^4.18.7",
    "js-cookie": "^2.2.1",
    "konva": "^7.1.3",
    "lodash": "^4.17.20",
    "nanoevents": "^5.1.8",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-click-outside": "^3.0.1",
    "react-color": "^2.18.0",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.0.1",
    "react-final-form": "^6.3.5",
    "react-hotkeys": "^2.0.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^6.0.2",
    "react-snap": "^1.23.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "roughjs": "^4.3.1",
    "shelljs": "^0.8.3",
    "simple-git": "^2.5.0",
